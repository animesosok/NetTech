<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="180.0" prefWidth="240.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nsu.networks.multicast_application.MulticastController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label fx:id="countTitle" />

    <Label fx:id="countLabel" />
   <AnchorPane prefHeight="66.0" prefWidth="200.0">
      <children>
          <Button fx:id="startButton" alignment="CENTER" layoutX="50.0" layoutY="44.0" onAction="#onStartButtonClick" prefHeight="43.0" prefWidth="101.0" text="START">
            <font>
               <Font name="System Bold" size="20.0" />
            </font></Button>
          <Button fx:id="finishButton" alignment="CENTER" layoutX="50.0" layoutY="44.0" onAction="#onFinishButtonClick" prefHeight="43.0" prefWidth="101.0" text="Finish" visible="false">
              <font>
                  <Font name="System Bold" size="20.0" />
              </font></Button>
      </children>
   </AnchorPane>
</VBox>
